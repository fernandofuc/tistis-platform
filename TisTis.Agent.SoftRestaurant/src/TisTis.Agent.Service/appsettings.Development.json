{
  "TisTisAgent": {
    "Api": {
      "BaseUrl": "http://localhost:3000",
      "ValidateSsl": false
    },

    "Sync": {
      "IntervalSeconds": 60
    },

    "Logging": {
      "MinimumLevel": "Debug"
    }
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    }
  }
}
